{
  "build": {
    "beforeBuildCommand": "cd client && yarn build",
    "beforeDevCommand": "cd client && yarn dev",
    "devPath": "http://localhost:3000",
    "distDir": "../client/dist"
  },
  "package": {
    "productName": "music-player",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "protocol": {
        "asset": true
      },
      "all": false,
      "dialog": {
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "music.player",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": "default-src 'self'; img-src 'self' asset: https://asset.localhost"
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "width": 1280,
        "height": 720,
        "resizable": false,
        "title": "music-player"
      }
    ]
  }
}
